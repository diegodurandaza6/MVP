<div id="AddUpdateportfolioPopup" class="modal fade" role="dialog"></div>
<div id="CandidateVedio" class="modal fade" role="dialog"></div>
<div id="Candidaterefences" class="modal fade" role="dialog"></div>
<div id="Candidatedeultimagepopup" class="modal fade" role="dialog"></div>


<main class="content">
    <div class="container-fluid p-0">
        <div class="row mb-2 mb-xl-4">
            <div class="col-auto d-none d-sm-block breadcrumb-1">
                <h3>
                    My Confidential Profile
                </h3>
            </div>
        </div>


        <div class="row res-msg">
            <div class="col-md-4 col-xl-3">

                <div class="conversation-tile-candidate">
                    <div class="card-header header-golden-bg position-relative">
                        <img src="@Model.FileName" class="conversation-img">
                        <h5 class="card-title mb-0 p-l-35">
                            Basic Information
                        </h5>
                    </div>
                    <div class="card-body bg-white">
                        <table class="table table-striped my-0 hr-l hr-r hr-b">
                            <tbody>

                                <tr>
                                    <td>
                                        <b>Name</b>:  ******
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Job Title</b>:
                                        @if (Model.Title.Length > 0)
                                        {
                                            <span>@Model.Title</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Email</b>:  ******
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Phone No.</b>:   ******
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Street Address</b>: ******
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>City/State</b>: ******
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Zip Code</b>:
                                        @if (Model.Zipcode.Length > 0)
                                        {
                                            <span>@Model.Zipcode</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
            <div class="col-md-8 col-xl-9">
                <div class="card mb-3 hr-t hr-l hr-r hr-b no-shadow">
                    <div class="card-header header-golden-bg">
                        <h5 class="card-title mb-0">Key Information</h5>
                    </div>
                    <hr class="m-0">
                    <div class="card-body p-b-0">
                        <div class="row dash-tile dash-tile-client-detail dash-tile-2-in-row">
                            <div>
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-user-graduate"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">Experience In Current Position</h5>
                                                @if (Model.experienceName.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2">@Model.experienceName</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-building"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Experience In Industry
                                                </h5>

                                                @if (Model.IndustryName.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2">@Model.IndustryName</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-dollar-sign"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Current Salary
                                                </h5>
                                                @if (Model.CurrentSalary.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2"> @Model.CurrentSalary</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-money-bill-wave"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Expected Salary
                                                </h5>
                                                @if (Model.expectedSalary.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2"> @Model.expectedSalary</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-graduation-cap"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Education Level
                                                </h5>
                                                @if (Model.educationlevel.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2">@Model.educationlevel</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div>
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="far fa-fw fa-file-archive"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Job Type
                                                </h5>
                                                @if (Model.jobtypeName.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2">@Model.jobtypeName</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-address-card"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Featured Confidential Profile
                                                </h5>

                                                @if (Model.Featured == 1)
                                                {
                                                    <p class="dash-tile-font-2">Yes </p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">No </p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-map-pin"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Relocation
                                                </h5>
                                                @if (Model.Relocation.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2">@Model.Relocation</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr class="m-0">
                    <div class="card-body p-b-0">
                        <div class="row dash-tile dash-tile-client-detail dash-tile-2-in-row">

                            <div class="desired-title-w">
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-crown"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Desired Title 1
                                                </h5>
                                                @if (Model.DesiredTitle1.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2">@Model.DesiredTitle1</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="desired-title-w">
                                <div class="card flex-fill custom-tile-jobdetail">
                                    <div class="card-body py-4">
                                        <div class="media">
                                            <div class="d-inline-block">
                                                <div class="stat">
                                                    <i class="fas fa-fw fa-crown"></i>
                                                </div>
                                            </div>
                                            <div class="media-body">
                                                <h5 class="mb-2 dash-tile-font">
                                                    Desired Title 2
                                                </h5>
                                                @if (Model.DesiredTitle2.Length > 0)
                                                {
                                                    <p class="dash-tile-font-2">@Model.DesiredTitle2</p>
                                                }
                                                else
                                                {
                                                    <p class="dash-tile-font-2">N/A</p>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
                <div class="card mb-3 hr-t hr-l hr-r hr-b no-shadow">
                    <div class="card-header header-golden-bg">
                        <h5 class="card-title mb-0">About Me</h5>
                    </div>

                    <hr class="m-0">
                    <div class="card-body p-b-0">

                        <div class="row">
                            <div class="card-body p-t-0">
                                @if (Model.PorfileDescerption.Length > 0)
                                {
                                    <p>
                                        @Model.PorfileDescerption
                                    </p>
                                }
                                else
                                {
                                    <p class="no-data-found">
                                        No Data Found...
                                    </p>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class=" ">
            <ul class="nav detail-tabs" role="tablist">
                <li class="">
                    <a class="active show" data-toggle="tab" href="#one" role="tab" aria-controls="nav-home" aria-selected="true">Education</a>
                </li>
                <li>
                    <a class="" data-toggle="tab" href="#two" role="tab" aria-controls="nav-profile" aria-selected="false">Experience</a>
                </li>
                <li>
                    <a class="" data-toggle="tab" href="#three" role="tab" aria-controls="nav-contact" aria-selected="false">Skills</a>
                </li>
                <li>
                    <a class="" data-toggle="tab" href="#four" role="tab" aria-controls="nav-contact" aria-selected="false">Scouting Report</a>
                </li>
                <li>
                    <a class="" data-toggle="tab" href="#five" role="tab" aria-controls="nav-contact" aria-selected="false">Licenses & Certifications</a>
                </li>
            </ul>
            <div class="card mb-3 hr-t hr-l hr-r hr-b no-shadow mb-4">
                <div class="card-body detail-pills">
                    <div class="fade active show" id="one" role="tabpanel">

                        <div class="row">
                            <div class="col-md-6">
                                <h4>
                                    Education
                                </h4>
                            </div>
                        </div>

                        <div class="lopping-tile">
                            <ul>
                                @if (Model.EducationCollection != null)
                                {
                                    foreach (var item in Model.EducationCollection)
                                    {
                                        if (item.visibleid == 1)
                                        {
                                            <li>
                                                <div>
                                                    <p class="head mr-3">
                                                        @item.Education
                                                    </p>
                                                    <p>
                                                        <i class="fas fa-fw fa-clock clr-golden"></i>
                                                        @(item.EducaStartYear == "" ? "N/A" : item.EducaStartYear)    - @(item.EducaEndYear == "" ? "N/A" : item.EducaEndYear)


                                                    </p>
                                                    <p>
                                                        <i class="fas fa-fw fa-user-graduate clr-golden"></i> @item.univarsity  <span class="text-underline">(@item.visibleName)</span>
                                                    </p>
                                                    <p>@item.description</p>
                                                </div>
                                            </li>

                                        }
                                    }
                                    if (Model.EducationCollection.Where(x => x.visibleid == 1).Count() < 1)
                                    {
                                        <span class="no-data-found">No Data found...</span>
                                    }
                                }
                                else
                                {
                                    <span class="no-data-found">No Data found...</span>
                                }
                            </ul>
                        </div>



                    </div>
                    <div class="fade" id="two" role="tabpanel">
                        <div class="row">
                            <div class="col-md-6">
                                <h4>
                                    Work Experience
                                </h4>
                            </div>
                        </div>

                        <div class="lopping-tile">
                            <ul>
                                @if (Model.workexperiencecollection != null)
                                {
                                    foreach (var item in Model.workexperiencecollection)
                                    {
                                        if (item.visibleid == 1)
                                        {

                                            <li>
                                                <div>
                                                    <p class="head mr-3">
                                                        Company Name: ******
                                                    </p>
                                                    <p>
                                                        <i class="far fa-fw fa-clock clr-golden"></i>   @(item.Todate == "" ? "N/A" : item.Todate)    - @(item.Fromdate == "" ? "N/A" : item.Fromdate)

                                                    </p>
                                                    <p>
                                                        <i class="fas fa-fw fa-user-graduate clr-golden"></i> @item.Title     <span class="text-underline">(@item.visibleName)</span>
                                                    </p>
                                                    <p>@Html.Raw(item.description)</p>
                                                </div>
                                            </li>

                                        }
                                    }
                                    if (Model.workexperiencecollection.Where(x => x.visibleid == 1).Count() < 1)
                                    {
                                        <span class="no-data-found">No Data found...</span>
                                    }
                                }
                                else
                                {
                                    <span class="no-data-found">No Data found...</span>
                                }
                            </ul>
                        </div>


                    </div>
                    <div class="fade" id="three" role="tabpanel">
                        <div class="row">
                            <div class="col-md-6">
                                <h4>
                                    Professional Skills
                                </h4>
                            </div>
                        </div>
                        <div class="skills">
                            <ul class="tags-new mb-2 mt-3">

                                @if (Model.SkillCollection != null)
                                {
                                    foreach (var item in Model.SkillCollection)
                                    {

                                        <li>
                                            <span>
                                                @item.skill
                                            </span>
                                        </li>
                                    }

                                }
                                else
                                {
                                    <span class="no-data-found">No Data found...</span>
                                }
                            </ul>
                        </div>
                    </div>
                    <div class="fade" id="four" role="tabpanel">
                        <div class="card-body p-0">
                            <div class="row">
                                <div class="col-md-6">
                                    <h4>
                                        Scouting Report
                                    </h4>
                                </div>
                            </div>
                            <div class="lopping-tile">
                                <ul>
                                    @if (Model.portfoliocecollection != null)
                                    {
                                        foreach (var item in Model.portfoliocecollection)
                                        {
                                            if (item.visibleid == 1)
                                            {
                                                <li>
                                                    <div>
                                                        <p class="scouting-heading p-0">
                                                           @item.description
                                                        </p>
                                                        <p>
                                                            <i class="fas fa-fw fa-user-graduate clr-golden"></i>  <span class="text-underline">(@item.visibleName)</span>
                                                        </p>
                                                    </div>
                                                </li>
                                            }
                                            if (Model.portfoliocecollection.Where(x => x.visibleid == 1).Count() < 1)
                                            {
                                                <span class="no-data-found">No Data found...</span>
                                            }
                                        }
                                    }
                                </ul>
                            </div>
                        </div>
                        
                    </div>
                    <div class="fade" id="five" role="tabpanel">
                        <div class="row">
                            <div class="col-md-6">
                                <h4>
                                    Licenses & Certifications
                                </h4>
                            </div>
                        </div>
                        <div class="lopping-tile">
                            <ul>
                                @if (Model.LicensesCertifications != null)
                                {
                                    foreach (var item in Model.LicensesCertifications)
                                    {
                                        if (item.visibleid == 1)
                                        {
                                            <li>
                                                <div>
                                                    <p class="head mr-3">
                                                        @item.Education
                                                    </p>
                                                    <p>
                                                        <i class="far fa-fw fa-clock clr-golden"></i>

                                                        @(item.year == "" ? "N/A" : item.year)    - @(item.EndYear == "" ? "N/A" : item.EndYear)

                                                         
                                                    </p>
                                                    <p>
                                                        <i class="fas fa-fw fa-user-graduate clr-golden"></i> @item.univarsity   <span class="text-underline">(@item.visibleName)</span>
                                                    </p>
                                                    <p>@item.description</p>
                                                </div>
                                            </li>
                                        }
                                        if (Model.LicensesCertifications.Where(x => x.visibleid == 1).Count() < 1)
                                        {
                                            <span class="no-data-found">No Data found...</span>
                                        }
                                    }

                                }
                                @if (Model.LicensesCertifications.Count() == 0)
                                {
                                    <span class="no-data-found">No Data found...</span>
                                }
                             
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>
<div>
    <i onclick="howitwork()"></i>
</div>
<div id="HowItworkpopup" class="modal fade" role="dialog"></div>
<script src="~/MvpTelent_js/Candidate/CandidateJs.js"></script>
<script>
    $('document').ready(function () {

        CandidateSkillAutoComplete(document.getElementById("skill"), '/Home/skillautocomplete', null);
    });
</script>
<script>
    var element = document.getElementById("confidentialprofile");
    element.classList.add("active");
</script>