@model Web.Model.Candidate.CandidateModel
@{
    ViewBag.Title = "confidentialprofile";
    Layout = "~/Views/Shared/_Candidatelayout.cshtml";
}
<script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
<link href="~/MvpTelent_js/autocomplete.css" rel="stylesheet" />
<script src="~/MvpTelent_js/autocomplete.js"></script>
<div id="candidateprofile_popup" class="modal fade" role="dialog"></div>
<div id="candidateprofilepopup" class="modal fade" role="dialog"></div>
<div id="Candidatecupdateprofile_PopUp" class="modal fade" role="dialog"></div>
<div id="candidateeducation_PopUp" class="modal fade" role="dialog"></div>
<div id="candidateworkexperience_PopUp" class="modal fade" role="dialog"></div>
<div id="candidateawrds_PopUp" class="modal fade" role="dialog"></div>
