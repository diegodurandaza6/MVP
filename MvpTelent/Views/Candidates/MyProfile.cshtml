                        </div>

                        <div class="card-body p-0">
                            <div class="lopping-tile">
                                <ul>
                                    @if (Model.portfoliocecollection != null)
                                    {
                                        foreach (var item in Model.portfoliocecollection)
                                        {
                                            <li>
                                                <div>
                                                    <div class="scouting-icon">
                                                        <a data-toggle="tooltip" data-placement="top" data-original-title="Delete" class="pull-right" onclick="Candidateportfoliopopup(@item.PortfolioId,@Model.UserId)">
                                                            <i class="far fa-fw fa-trash-alt"></i>
                                                        </a>
                                                        <a data-toggle="tooltip" data-placement="top" data-original-title="Edit" class="pull-right mr-2" onclick="CandidateportfolioDelete(@item.PortfolioId)">
                                                            <i class="far fa-fw fa-edit"></i>
                                                        </a>
                                                    </div>
                                                    <p class="scouting-heading">
                                                        @Html.Raw(item.description)

                                                    </p>
                                                    <p>
                                                        <i class="fas fa-fw fa-user-graduate clr-golden"></i>  <span class="text-underline">(@item.visibleName)</span>
                                                    </p>
                                                </div>
                                            </li>
                                        }
                                    }
                                    @if (Model.portfoliocecollection == null)
                                    {
                                        <span class="no-data-found">No Data found...</span>
                                    }
                                </ul>
                            </div>
                        </div> 

                    </div>
                    <div class="fade" id="five" role="tabpanel">
                        <div class="row">
                            <div class="col-md-6">
                                <h4>
                                    Licenses & Certifications
                                </h4>
                            </div>
                            <div class="col-md-6 text-right">
                                <a class="new-btn" onclick="LicensesCertificationspopup(0,@Model.UserId)" data-toggle="tooltip" data-placement="top" data-original-title="Add">
                                    Add
                                </a>
                            </div>
                        </div>
                        <div class="lopping-tile">
                            <ul>
                                @if (Model.LicensesCertifications != null)
                                {
                                    foreach (var item in Model.LicensesCertifications)
                                    {
                                        <li>
                                            <div>
                                                <p class="head mr-3">
                                                    @item.Education
                                                    <a data-toggle="tooltip" data-placement="top" data-original-title="Edit" class="pull-right" onclick="LicensesCertificationspopup(@item.Id,@Model.UserId)">
                                                        <i class="align-middle" data-feather="edit"></i>
                                                    </a>
                                                    <a data-toggle="tooltip" data-placement="top" data-original-title="Delete" class="pull-right mr-2" onclick="LicensesCertificationsDelete(@item.Id)">
                                                        <i class="align-middle" data-feather="trash"></i>
                                                    </a>
                                                </p>
                                                <p>
                                                    <i class="far fa-fw fa-clock clr-golden"></i>

                                                    @(item.year == "" ? "N/A" : item.year)    - @(item.EndYear == "" ? "N/A" : item.EndYear)

                                                </p>
                                                <p>
                                                    <i class="fas fa-fw fa-user-graduate clr-golden"></i> @item.univarsity   <span class="text-underline">(@item.visibleName)</span>
                                                </p>
                                                <p>@item.description</p>
                                            </div>
                                        </li>

                                    }
                                }
                                @if (Model.LicensesCertifications != null)
                                {
                                    <span class="no-data-found">No Data found...</span>
                                }
                                @*else
                                    {
                                        <span class="no-data-found">No Data found...</span>
                                    }*@
                            </ul>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
