<main class="content">
    <div class="container-fluid p-0">
        <div class="row mb-2 mb-xl-4 position-relative">
            <div class="col-auto d-none d-sm-block breadcrumb-1">
                <h3>My Detailed Profile</h3>
            </div>
            <div class="breadcum-2">
                <a class="btn-new-1" onclick="candidateprofile_popup(@Model.Id)">
                    Edit Profile
                </a>
                <a class="btn-new-1" onclick="Candidatedefaultimage(@Model.Id)">
                    Change Profile Pic
                </a>
            </div>
        </div>

        <div class="row res-msg mb-3">
            <div class="col-md-4 col-xl-3">

                <div class="conversation-tile-candidate">
                    <div class="card-header header-golden-bg position-relative">
                        <img src="@Model.FileName" class="conversation-img">
                        <h5 class="card-title mb-0 p-l-35">
                            Basic Information
                        </h5>
                    </div>
                    <div class="card-body bg-white">
                        <table class="table table-striped my-0 hr-l hr-r hr-b">
                            <tbody>

                                <tr>
                                    <td>
                                        <b>Name</b>: @Model.Name  @Model.LastName
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Job Title</b>:
                                        @if (Model.Title != null)
                                        {
                                            <span>@Model.Title</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Email</b>: @Model.Email
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Phone No.</b>:
                                        @if (Model.Phone != null)
                                        {
                                            <span>@Model.Phone</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Street Address</b>:
                                        @if (Model.Address1 != null)
                                        {
                                            <span>@Model.Address1</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>City/State</b>:
                                        @if (Model.location != null)
                                        {
                                            <span>@Model.location</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Zip Code</b>:
                                        @if (Model.Zipcode != null)
                                        {
                                            <span>@Model.Zipcode</span>
                                        }
                                        else
                                        {
                                            <span>N/A</span>
                                        }
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="card mb-3 hr-t hr-l hr-r hr-b no-shadow">
                    <div class="card-header header-golden-bg" ">
                        <h5 class="card-title mb-0">My Resume</h5>
                    </div>
                    <hr class="my-0">
                    <div class="card-body">
                        <label>
                            Upload Resume
                        </label><br />
