        </div>
        <div class=" ">
            <ul class="nav detail-tabs" role="tablist">
                <li class="">
                    <a id="tab_1" onclick="ActiveTab(1)" class="" data-toggle="tab" href="#one" role="tab" aria-controls="nav-home" aria-selected="true">Education</a>
                </li>
                <li>
                    <a id="tab_2" onclick="ActiveTab(2)" class="" data-toggle="tab" href="#two" role="tab" aria-controls="nav-profile" aria-selected="false">Experience</a>
                </li>
                <li>
                    <a id="tab_3" onclick="ActiveTab(3)" class="" data-toggle="tab" href="#three" role="tab" aria-controls="nav-contact" aria-selected="false">Skills</a>
                </li>
                <li>
                    <a id="tab_4" onclick="ActiveTab(4)" class="" data-toggle="tab" href="#four" role="tab" aria-controls="nav-contact" aria-selected="false">Scouting Report</a>
                </li>
                <li>
                    <a id="tab_5" onclick="ActiveTab(5)" class="" data-toggle="tab" href="#five" role="tab" aria-controls="nav-contact" aria-selected="false">Licenses & Certifications</a>
                </li>
            </ul>
            <div class="card mb-3 hr-t hr-l hr-r hr-b no-shadow mb-4">
                <div class="card-body detail-pills">
                    <div class="fade " id="one" role="tabpanel">

                        <div class="row">
                            <div class="col-md-6">
                                <h4>
                                    Education
                                </h4>
                            </div>
                            <div class="col-md-6 text-right">
                                <a class="new-btn" onclick="CandidateEducationpopup(0,@Model.UserId)" data-toggle="tooltip" data-placement="top" data-original-title="Add">
                                    Add
                                </a>
                            </div>
                        </div>

                        <div class="lopping-tile">
                            <ul>

                                @if (Model.EducationCollection != null)
                                {
                                    foreach (var item in Model.EducationCollection)
                                    {
                                        <li>
                                            <div>
                                                <p class="head mr-3">
                                                    @item.Education
                                                    <a data-toggle="tooltip" data-placement="top" data-original-title="Edit" class="pull-right" onclick="CandidateEducationpopup(@item.educationId,@Model.UserId)">
                                                        <i class="align-middle" data-feather="edit"></i>
                                                    </a>
                                                    <a data-toggle="tooltip" data-placement="top" data-original-title="Delete" class="pull-right mr-2" onclick="CandidateEducationDelete(@item.educationId)">
                                                        <i class="align-middle" data-feather="trash"></i>
                                                    </a>
                                                </p>
                                                <p>
                                                    <i class="fas fa-fw fa-clock clr-golden"></i>
                                                    @(item.EducaStartYear == "" ? "N/A" : item.EducaStartYear)    - @(item.EducaEndYear == "" ? "N/A" : item.EducaEndYear)



                                                </p>
                                                <p>
                                                    <i class="fas fa-fw fa-user-graduate clr-golden"></i> @item.univarsity  <span class="text-underline">(@item.visibleName)</span>
                                                </p>
                                                <p>
                                                    @item.description
                                                </p>
                                            </div>
                                        </li>

                                    }

                                }
                                @if (Model.EducationCollection == null)
                                {
                                    <span class="no-data-found">No Data found...</span>
                                }
                                @*else
                                    {
                                        <span class="no-data-found">No Data found...</span>
                                    }*@

                            </ul>
                        </div>

                    </div>
                    <div class="fade" id="two" role="tabpanel">
                        <div class="row">
                            <div class="col-md-6">
                                <h4>
                                    Work Experience
                                </h4>
                            </div>
                            <div class="col-md-6 text-right">
                                <a class="new-btn" onclick="Candidateworkexperiencepopup(0,@Model.UserId)" data-toggle="tooltip" data-placement="top" data-original-title="Add">
                                    Add
                                </a>
                            </div>
                        </div>
                        <div class="lopping-tile">
                            <ul>


                                @if (Model.workexperiencecollection != null)
                                {
                                    foreach (var item in Model.workexperiencecollection)
                                    {


                                        <li>
                                            <div>
                                                <p class="head mr-3">
                                                    @item.Company
                                                    <a data-toggle="tooltip" data-placement="top" data-original-title="Edit" class="pull-right" onclick="Candidateworkexperiencepopup(@item.workId,@Model.UserId)">
                                                        <i class="align-middle" data-feather="edit"></i>
                                                    </a>
                                                    <a data-toggle="tooltip" data-placement="top" data-original-title="Delete" class="pull-right mr-2" onclick="CandidateworkexperienceDelete(@item.workId)">
                                                        <i class="align-middle" data-feather="trash"></i>
                                                    </a>
                                                </p>
                                                <p>
                                                    <i class="far fa-fw fa-clock clr-golden"></i>   @(item.Todate == "" ? "N/A" : item.Todate)    - @(item.Fromdate == "" ? "N/A" : item.Fromdate)
                                                    
                                                </p>

                                                <p>
                                                    Visibility : (@item.visibleName)
                                                </p>


                                                <p>
                                                    <i class="fas fa-fw fa-user-graduate clr-golden"></i> @item.Title
                                                </p>
                                                <p>   @Html.Raw(item.description)</p>
                                            </div>
                                        </li>

                                    }

                                }
