                            <div class="row">
                                <div class="col-md-4">
                                    <div id="skills-form">
                                        <label>
                                            Professional Skills
                                        </label>
                                        <div id="skillform">
                                            <table class="w-100">
                                                <tbody>
                                                    <tr>
                                                        <td class="position-relative">
                                                            <input type="text" autocomplete="off" onkeyup="checkskill()" id="skill" name="skill" class="form-control" placeholder="Please enter skill" maxlength="30" />
                                                        </td>
                                                        <td style="width: 37px;">
                                                            <button class="btn btn-primary border-0 p-t-8 p-b-8 round-3px" onclick="SkillAddUpdate(0,@Model.UserId)">Save</button>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td colspan="2">
                                                            <label class="error" id="errormessage" name="errormessage">

                                                            </label>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <ul class="tags-new mb-2 mt-3">
                                @if (Model.SkillCollection != null)
                                {
                                    foreach (var item in Model.SkillCollection)
                                    {
                                        <li>
                                            <span>
                                                @item.skill
                                            </span>
                                            <span>
                                                <a onclick="deleteskill(@item.Id)" data-toggle="tooltip" data-placement="top" data-original-title="Remove">
