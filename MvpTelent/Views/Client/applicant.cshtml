                                            <a onclick="selectJobId('Please select a job','00','')" class="dropdown-toggle" role="button" name="jobLocationid" id="jobLocationid" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                @(Model.Title == null ? "Please select a job" : Model.Title)
                                            </a>
                                            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuLink">
                                                <a class="dropdown-item" onclick="selectJobId('Please select a job','','')">Please select a job </a>
                                                @if (Model.JobCollection != null)
                                                {
                                                    foreach (var item in Model.JobCollection)
                                                    {
                                                        <a class="dropdown-item" onclick="selectJobId('@item.Name', '@item.Id', '@item.Location')">
                                                            <span>@item.Name</span> <span> @item.Location.Replace(", USA", "")</span>
                                                        </a>
                                                    }
                                                }
                                            </div>

