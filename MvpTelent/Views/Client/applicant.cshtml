                                    </div>
                                    <div class="col-md-8 m-b-10">

                                        @*@Html.DropDownListFor(m => m.JobId, new SelectList(Model.JobCollection, "Id", "Name"), "Select Job", new { @class = "custom-select", @Id = "JobId", @Name = "JobId", @onchange = "GetClient_JobOnwerDetailById(this.value)" })*@

                                        <div style="display:none" >
                                            @Html.DropDownListFor(m => m.JobId, new SelectList(Model.JobCollection, "Id", "Name"), "Select Job", new { @class = "custom-select", @Id = "JobId", @Name = "JobId" })
