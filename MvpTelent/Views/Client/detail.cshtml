@model Web.Model.Candidate.CandidateModel
@{
    ViewBag.Title = "Candidate Detail";
    Layout = "~/Views/Shared/_ClientLayout.cshtml";
}
<div id="CreateInterview_PopUp" class="modal fade" role="dialog"></div>
<div id="InterviewStatusByid_Popup" class="modal fade" role="dialog"></div>
<main class="content">
    <div class="container-fluid p-0">
        <div class="row mb-2 mb-xl-4 position-relative">
            <div class="col-auto d-none d-sm-block breadcrumb-1">
                <h3>
                    Candidate Detail
                </h3>
            </div>
            <div class="breadcum-2">
                @if (Model.JobId > 0)
                {
                    <a href="/Client/applicant" class="btn-new-1 mr-6">
                        Back to Applicants Pipeline
                    </a>
                }
                <div class="dropdown scouting-dropdown top-4px">
                    <a href="#" data-toggle="dropdown" data-display="static" aria-expanded="false">
                        <i class="fas fa-fw fa-bars" aria-hidden="true"></i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right">
                        @if (Model.resumeName != "")
                        {
                            <a href="@Model.Resumefile" class="dropdown-item">
                                <i class="fa fa-download acs" aria-hidden="true"></i> Download CV
                            </a>
                        }
                        else
                        {
                            <a onclick="resumealertmassege()" class="dropdown-item">
                                <i class="fa fa-download acs" aria-hidden="true"></i> Download CV
                            </a>
                        }
                        <a onclick="InterviewSchedulepopup(0,@Model.Id,@Model.JobId)" class="dropdown-item">
                            <i class="far fa-fw fa-calendar-alt"></i> Schedule interview
                        </a>
                        <a onclick="interviewStatusPopup(@Model.Id,@Model.InterviewSId,@Model.JobId)" class="dropdown-item">
                            <i class="fas fa-fw fa-exchange-alt"></i> Change Pipeline status
                        </a>
                        <a onclick="Clientnotepopup(@Model.Id,@Model.JobId)" class="dropdown-item">
                            <i class="far fa-fw fa-file-alt"></i> Add Notes
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="card mb-3 hr-t hr-l hr-r hr-b no-shadow">
                    <div class="card-header header-golden-bg">
                        <h5 class="card-title mb-0">Basic Information</h5>
                    </div>
                    <div class="card-body p-b-0">
                        <div class="row position-relative">
                            <div class="p-profile mb-3 row-mg w-100 m-0">
                                <div class="p-profile-body col-md-12 pt-0 pl-3">
                                    <table class="table table-striped hr-b hr-l hr-r m-0">
                                        <tbody>
                                            <tr>
                                                <td rowspan="3" style="width: 146px;">
                                                    <div class="p-profile-body-2 pb-0 pt-0" style="width: 122px;">
                                                        <img src="@Model.FileName" class="p-profile-img p-0 bg-white hr-l hr-r hr-b hr-t">
                                                    </div>
                                                </td>
                                                <td><b>Name</b> :</td>
                                                <td> @Model.Name @Model.LastName</td>
                                            </tr>
                                            <tr>
                                                <td><b>Phone No</b> :</td>
                                                <td>
                                                    @if (Model.Phone == "")
                                                    {
                                                        <span>N/A</span>
                                                    }
                                                    else
                                                    {
                                                        @Model.Phone
                                                    }
                                                </td>
                                            </tr>
                                            <tr>
                                                <td><b>Email</b> :</td>
                                                <td>
                                                    @Model.Email
                                                </td>
                                            </tr>
                                            <tr>
                                                <td><b>Street Address</b> :</td>
                                                @if (Model.Address1 != null)
                                                {
                                                    <td colspan="2">
                                                        @Model.Address1
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td colspan="2">
                                                        N/A
                                                    </td>
                                                }
                                            </tr>
                                            <tr>
                                                <td><b>City/State</b> :</td>
                                                @if (Model.location != null)
                                                {
                                                    <td colspan="2">
                                                        @Model.location
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td colspan="2">
                                                        N/A
                                                    </td>
                                                }
                                            </tr>
                                            <tr>
                                                <td><b>Job Title</b> :</td>
                                                @if (Model.Title != null)
                                                {
                                                    <td colspan="2">
                                                        @Model.Title
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td colspan="2">
                                                        N/A
                                                    </td>
                                                }
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card flex-fill hr-t hr-l hr-r hr-b no-shadow">
                    <div class="card-header hr-b-1 header-golden-bg">
                        <h5 class="card-title mb-0">
                            Meta Information
                        </h5>
                    </div>
