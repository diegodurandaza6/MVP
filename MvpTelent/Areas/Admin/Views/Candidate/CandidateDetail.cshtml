<main class="content">
    <div class="container-fluid p-0">
        <div class="row mb-2 mb-xl-4 position-relative">
            <div class="col-auto d-none d-sm-block breadcrumb-1">
                <h3>Edit Condidate</h3>
               
            </div>
            <div class="breadcum-2">
                <div class="dropdown scouting-dropdown top-4px">
                    <a href="#" data-toggle="dropdown" data-display="static" aria-expanded="false">
                        <i class="fas fa-fw fa-bars" aria-hidden="true"></i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right">
                        <a class="dropdown-item" onclick="GroupMove(@Model.Id)"><i class="fas fa-fw fa-user-plus"></i> Add in Group</a>
                        @if (Model.resumeName != "")
                        {
                            <a href="@Model.Resumefile" class="dropdown-item">
                                <i class="fa fa-download acs" aria-hidden="true"></i> Download CV
                            </a>
                        }
                        else
                        {
                            <a onclick="resumealertmassege()" class="dropdown-item">
                                <i class="fa fa-download acs" aria-hidden="true"></i> Download CV
                            </a>
                        }
                        <a class="dropdown-item" onclick="Candidatedefaultimage(@Model.Id)">
                            <i class="far fa-fw fa-image"></i> Change Profile Image
                        </a>
                        <a class="dropdown-item position-relative">
                            <input id="Uploadfiles" type="file" onchange="ResumeUpload(@Model.Id)" class="hamburger-choose-file">
                            <i class="fas fa-fw fa-upload"></i> Upload Resume
                        </a>
                        <a class="dropdown-item" onclick="CandidatesharedJobpopup('@EncryptDecrypt.Decrypt(Convert.ToString(Model.Ids))')">
                            <i class="far fa-fw fa-image"></i> Submit this Profile
                        </a>
                        <a class="dropdown-item" onclick="candidateprofilepopup(@Model.Id)">
                            <i class="far fa-fw fa-edit"></i> Edit Candidate Profile
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="card flex-fill">
                    <div class="card-header hr-b header-golden-bg">
                        <h5 class="card-title mb-0">
                            Candidate Detail

                        </h5>
                    </div>

                    <div class="card-body p-b-0">
                        <div class="row position-relative">
                            <div class="p-profile mb-3 row-mg w-100 m-0">
                                <div class="p-profile-body col-md-12 pt-0 pl-3">
                                    <table class="table table-striped hr-b hr-l hr-r m-0">
                                        <tbody>
                                            <tr>
                                                <td rowspan="3" style="width: 146px;">
                                                    <div class="p-profile-body-2 pb-0 pt-0" style="width: 122px;">
                                                        <img src="@Model.FileName" class="p-profile-img p-0 bg-white hr-l hr-r hr-b hr-t">
                                                    </div>
                                                </td>
                                                <td><b>Name</b> :</td>
                                                <td> @Model.Name @Model.LastName</td>
                                            </tr>
                                            <tr>
                                                <td><b>Phone No</b> :</td>
                                                @if (Model.Phone.Length > 0)
                                                {
                                                    <td>
                                                        @Model.Phone
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td>
                                                        N/A
                                                    </td>
                                                }
                                            </tr>
                                            <tr>
                                                <td><b>Email</b> :</td>
                                                <td>
                                                    @Model.Email
                                                </td>
                                            </tr>
                                            <tr>
                                                <td><b>Job Title</b> :</td>
                                                @if (Model.Title.Length > 0)
                                                {
                                                    <td colspan="2">
                                                        @Model.Title
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td colspan="2">
                                                        N/A
                                                    </td>
                                                }
                                            </tr>
                                            <tr>
                                                <td><b>Street Address</b> :</td>
                                                @if (Model.Address1.Length > 0)
                                                {
                                                    <td colspan="2">
                                                        @Model.Address1
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td colspan="2">
                                                        N/A
                                                    </td>
                                                }
                                            </tr>
                                            <tr>
                                                <td><b>City/State</b> :</td>
                                                @if (Model.location.Length > 0)
                                                {
                                                    <td colspan="2">
                                                        @Model.location
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td colspan="2">
                                                        N/A
                                                    </td>
                                                }
                                            </tr>
                                            <tr>
                                                <td><b>Resume</b> :</td>
                                                <td colspan="2">
                                                    @if (Model.resumeName != "")
                                                    {
                                                        <a class="active-clr-2 mr-2" href="@Model.Resumefile" data-toggle="tooltip" data-placement="top" data-original-title="Download Resume" target="_blank">
                                                            <i class="fa fa-download acs" aria-hidden="true"></i> Download Resume
                                                        </a>
                                                    }
                                                    else
                                                    {
                                                        <a class="active-clr-2 mr-2" onclick="resumealertmassege()" data-toggle="tooltip" data-placement="top" data-original-title="Download Resume">
                                                            <i class="fa fa-download acs" aria-hidden="true"></i> Download Resume
                                                        </a>
                                                    }
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card-body pb-0 pt-1">
                        <div class="skills">
                            <div>
                                <label>
                                    Candidate Tag
                                </label>
                                <table class="w-100">
                                    <tbody>
                                        <tr>
                                            <td>
                                                <div class="autocomplete">
                                                    <input type="text" name="CandidateTagId" maxlength="40" id="CandidateTagId" placeholder="Please enter tag" autocomplete="off" class="form-control">
                                                </div>
                                                <div id="autocompletedata"></div>
                                            </td>
                                            <td style="width: 37px;">
                                                <button type="button" class="btn btn-primary border-0 p-t-8 p-b-8 round-3px" onclick="Candidatetagvalue(@Model.Id)">Save</button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <ul class="tags-new mb-2 mt-3">
                                @if (Model.candidatetagviewcollection != null)
                                {
                                    foreach (var item in Model.candidatetagviewcollection)
                                    {
                                        <li>
                                            <span>
                                                @item.CandidateTag
                                            </span>
                                            <span>
                                                <a onclick="deletecandidatetag(@item.Id)">
                                                    <i class="fa fa-times" aria-hidden="true"></i>
                                                </a>
                                            </span>
                                        </li>
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                    <hr class="mb-0" />
                </div>
            </div>
            <div class="col-md-4">
