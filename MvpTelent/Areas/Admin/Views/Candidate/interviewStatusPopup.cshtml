@model Web.Model.Candidate.CandidateModel
<div class="modal-dialog modal-sm" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel1">Change Pipeline Status</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
      
                    <form id="clientform" name="form-validation" method="POST">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <input type="hidden" name="H_ContactIs" id="H_ContactIs" />
                                    <input type="hidden" name="h_tId" id="h_tId" />
                                    <label class="font-w-600">Pipeline Status </label>
                                    @Html.DropDownListFor(m => m.Id, new SelectList(Model.InterviewStaus, "Id", "Name"), "Select Status", new { @class = "form-control", @Id = "InterviewSId", @Name = "InterviewSId" })
                                </div>
                            </div>
                        </div>
                    </form>
            
        </div>
        <div class="modal-footer">
            <input type="button" name="interviewStatus" id="interviewStatus" class="btn btn-primary" style="width:45%;" onclick="Interviewstatus(@Model.CandidateId,@Model.JobId)" value="Change Status">
            <button type="button" class="btn btn-light" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>